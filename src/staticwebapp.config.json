{
    "trailingSlash": "auto",
    "routes": [
      {
        "route": "/src*",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/login",
        "rewrite": "/.auth/login/github"
      },
      {
        "route": "/logout",
        "redirect": "/.auth/logout"
      }
    
    ]
}
